{
    "family": "project2-backend-task",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::523026302352:role/project2-ecs-exec-role",
    "taskRoleArn": "arn:aws:iam::523026302352:role/project2-ecs-task-role",
    "containerDefinitions": [
        {
            "name": "project2-backend-container",
            "image": "PLACEHOLDER",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "ENVIRONMENT",
                    "value": "production"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/project2-backend",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}